{% from "./crmEvidence.njk" import crmEvidence %}
{% from "./crm4/crm4AdditionalExpenditure.njk" import crm4AdditionalExpenditure %}
{% from "./crm4/crm4AlternativeQuotes.njk" import crm4AlternativeQuotes %}
{% from "./crm7/crm7CaseDisposal.njk" import crm7CaseDisposal %}

{% macro customDisplay(params) %}
    {% if params.name == 'crmEvidence' %}
        {{ crmEvidence(params.value) }}
    {% elif params.name == 'crm4AdditionalExpenditure' %}
        {{ crm4AdditionalExpenditure(params.value) }}
    {% elif params.name == 'crm4AlternativeQuotes' %}
        {{ crm4AlternativeQuotes(params.value) }}
    {% elif params.name == 'crm7CaseDisposal' %}
        {{ crm7CaseDisposal(params.value) }}
    {% endif %}

{% endmacro %}
